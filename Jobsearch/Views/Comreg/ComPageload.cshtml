@model Jobsearch.Models.comInsert
@{
    ViewBag.Title = "ComPageload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Company Registration Form</h1>
<div>
    @using (Html.BeginForm("InsertCom_click", "Comreg", FormMethod.Post))
    {
        <div>
            <label>Name :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.name)
            @Html.ValidationMessageFor(m => m.name)
        </div>
        <div>
            <label>Address :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.address)
            @Html.ValidationMessageFor(m => m.address)
        </div>
        <div>
            <label>Email :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.email)
            @Html.ValidationMessageFor(m => m.email)
        </div>
        <div>
            <label>Phone :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.phone)
            @Html.ValidationMessageFor(m => m.phone)
        </div>
        <div>
            <label>Website :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.website)
            @Html.ValidationMessageFor(m => m.website)
        </div>
        <div>
            <label>Username :</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.username)
        </div>
        <div>
            <label>Password :</label>
        </div>
        <div>
            @Html.PasswordFor(m => m.pass)
        </div>
        <div>
            <label>Conform Password :</label>
        </div>
        <div>
            @Html.PasswordFor(m => m.cpassword)
            @Html.ValidationMessageFor(m => m.cpassword)
        </div>
        <div>
            <input type="submit" value="Insert" />
            @Html.DisplayFor(m => m.adminmsg)
        </div>
    }
</div>

